{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "requestify": {
      "type": "library",
      "root": "libs/requestify",
      "entryFile": "index",
      "sourceRoot": "libs/requestify/src",
      "compilerOptions": {
        "tsConfigPath": "libs/requestify/tsconfig.lib.json"
      }
    },
    "nestlog": {
      "type": "library",
      "root": "libs/nestlog",
      "entryFile": "index",
      "sourceRoot": "libs/nestlog/src",
      "compilerOptions": {
        "tsConfigPath": "libs/nestlog/tsconfig.lib.json"
      }
    }
  }
}